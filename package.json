{
  "name": "health-dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:lan": "next dev -H 0.0.0.0 -p 3000",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "env:setup": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' scripts/setup-env.ts",
    "db": "npm run db:up",
    "db:init": "npm run env:setup && npm run prisma:generate && prisma migrate dev -n init",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:seed": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' prisma/seed.ts",
    "db:up": "docker compose up -d db",
    "db:down": "docker compose down",
    "db:check": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' scripts/db-check.ts"
  },
  "dependencies": {
    "@nivo/bar": "^0.85.1",
    "@nivo/line": "^0.85.1",
    "@prisma/client": "^6.15.0",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-slot": "^1.0.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "lucide-react": "^0.363.0",
    "next": "14.1.4",
    "react": "^18",
    "react-dom": "^18",
    "tailwind-merge": "^2.2.2",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.1.4",
    "postcss": "^8",
    "prisma": "^6.15.0",
    "tailwindcss": "^3.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
